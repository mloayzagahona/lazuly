<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE application SYSTEM "dtds/application.dtd">

<application name="conference"> 
	
	<!-- 
	A default module for each entity is assumed with the
	controllers on <default-module/>
	-->
	
	<default-module>		  
		<controller name="Typical"/>		
	</default-module>
	
<!-- entity : country -->
  <module name="Country">
	  <model name="Country"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : role -->
  <module name="Role">
	  <model name="Role"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : address -->
  <module name="Address">
	  <model name="Address"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : conference -->
  <module name="Conference">
	  <model name="Conference"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : conference_feedback -->
  <module name="ConferenceFeedback">
	  <model name="ConferenceFeedback"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : conference_member -->
  <module name="ConferenceMember">
	  <model name="ConferenceMember"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : evaluation -->
  <module name="Evaluation">
	  <model name="Evaluation"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : member_role -->
  <module name="MemberRole">
	  <model name="MemberRole"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : presentation -->
  <module name="Presentation">
	  <model name="Presentation"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : presentation_place -->
  <module name="PresentationPlace">
	  <model name="PresentationPlace"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : speaker -->
  <module name="Speaker">
	  <model name="Speaker"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : speaker_presentation -->
  <module name="SpeakerPresentation">
	  <model name="SpeakerPresentation"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : sponsor -->
  <module name="Sponsor">
	  <model name="Sponsor"/>  
	  <env-var name="XAVA_SEARCH_ACTION" value="MpTypical.searchByViewKey"/>
	  <controller name="MpTypical"/>
	</module>	
<!-- entity : stat_mb_by_role -->
  <module name="MemberPerRoleCountryAndConference">
	  <model name="MemberPerRoleCountryAndConference"/>  
	  <controller name="MpSearchAndPrint"/>
	</module>	
<!-- entity : stat_mb_per_ctry_conf -->
  <module name="MemberPerCountryAndConference">
	  <model name="MemberPerCountryAndConference"/>  
	  <controller name="MpSearchAndPrint"/>
	</module>	
	
		
</application>


